{
  "extends": ["airbnb-typescript"],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "rules": {
    "no-unused-vars": "off",
    "eol-last": "off",
    "lines-between-class-members": "off",
    "class-methods-use-this": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  }
}